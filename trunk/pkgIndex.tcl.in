package ifneeded gpg @PACKAGE_VERSION@ \
        "catch {load [file join [list $dir] @PKG_LIB_FILE@]}
         source [file join [list $dir] tclgpg.tcl]
         package provide gpg @PACKAGE_VERSION@"

